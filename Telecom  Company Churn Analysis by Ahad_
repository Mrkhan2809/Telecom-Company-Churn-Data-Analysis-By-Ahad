   "source": [
    "# converted 0 and 1 values of senior citizen to yes/no to make it easier to understand"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "608a1b90-5d12-40a7-816f-b313fe27df43",
   "metadata": {},
   "outputs": [],
   "source": [
    "def conv(value):\n",
    "    if value ==1:\n",
    "        return \"YES\"\n",
    "    else:\n",
    "        return\"NO\"\n",
    "\n",
    "df[\"SeniorCitizen\"] = df[\"SeniorCitizen\"].apply(conv)\n"
   ]
  },
